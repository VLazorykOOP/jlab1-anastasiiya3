
            
        import java.util.ArrayList;
import java.util.List;
import java.util.StringTokenizer;

public class LexicographicalWordsFinder {

    public static void main(String[] args) {
        // –ü—Ä–∏–∫–ª–∞–¥ —Ç–µ–∫—Å—Ç—É –∑ –º–∞–ª–∏—Ö –ª–∞—Ç–∏–Ω—Å—å–∫–∏—Ö –ª—ñ—Ç–µ—Ä, —Ä–æ–∑–¥—ñ–ª–µ–Ω–∏—Ö –ø—Ä–æ–±—ñ–ª–∞–º–∏ —Ç–∞ —Ä–æ–∑–¥—ñ–ª–æ–≤–∏–º–∏ –∑–Ω–∞–∫–∞–º–∏
        String text = "ab.book.cellar.loop,additionally;test,hello-aab-zoo-aaab.";

        List<String> result = findLexicographicalWords(text);

        System.out.println("‚úÖ –û—Ä–∏–≥—ñ–Ω–∞–ª—å–Ω–∏–π —Ç–µ–∫—Å—Ç:");
        System.out.println("  " + text);
        
        System.out.println("\n--- –†–µ–∑—É–ª—å—Ç–∞—Ç ---");
        if (result.isEmpty()) {
            System.out.println("–ù–µ –∑–Ω–∞–π–¥–µ–Ω–æ —Å–ª—ñ–≤, –ª—ñ—Ç–µ—Ä–∏ –≤ —è–∫–∏—Ö —Ä–æ–∑–º—ñ—â–µ–Ω—ñ –≤ –ª–µ–∫—Å–∏–∫–æ–≥—Ä–∞—Ñ—ñ—á–Ω–æ–º—É –ø–æ—Ä—è–¥–∫—É.");
        } else {
            System.out.println("üëâ –°–ª–æ–≤–∞ –∑ –ª—ñ—Ç–µ—Ä–∞–º–∏ –≤ –ª–µ–∫—Å–∏–∫–æ–≥—Ä–∞—Ñ—ñ—á–Ω–æ–º—É –ø–æ—Ä—è–¥–∫—É:");
            for (String word : result) {
                System.out.println("  " + word);
            }
        }
    }

    /**
     * –ó–Ω–∞—Ö–æ–¥–∏—Ç—å –≤—Å—ñ —Å–ª–æ–≤–∞ –≤ —Ç–µ–∫—Å—Ç—ñ, –ª—ñ—Ç–µ—Ä–∏ –≤ —è–∫–∏—Ö —Ä–æ–∑–º—ñ—â–µ–Ω—ñ –≤ –ª–µ–∫—Å–∏–∫–æ–≥—Ä–∞—Ñ—ñ—á–Ω–æ–º—É –ø–æ—Ä—è–¥–∫—É.
     * @param text –í—Ö—ñ–¥–Ω–∏–π —Ç–µ–∫—Å—Ç.
     * @return –°–ø–∏—Å–æ–∫ —Å–ª—ñ–≤, —â–æ –≤—ñ–¥–ø–æ–≤—ñ–¥–∞—é—Ç—å —É–º–æ–≤—ñ.
     */
    public static List<String> findLexicographicalWords(String text) {
        List<String> lexicographicalWords = new ArrayList<>();
        
        // –í–∏–∫–æ—Ä–∏—Å—Ç–æ–≤—É—î–º–æ StringTokenizer –∞–±–æ split –¥–ª—è —Ä–æ–∑–¥—ñ–ª–µ–Ω–Ω—è —Ç–µ–∫—Å—Ç—É –Ω–∞ —Å–ª–æ–≤–∞.
        // StringTokenizer –∫—Ä–∞—â–µ –ø—ñ–¥—Ö–æ–¥–∏—Ç—å –¥–ª—è –æ–±—Ä–æ–±–∫–∏ –¥–µ–∫—ñ–ª—å–∫–æ—Ö —Ä–æ–∑–¥—ñ–ª—å–Ω–∏–∫—ñ–≤.
        String delimiters = " .,;:-"; 
        StringTokenizer tokenizer = new StringTokenizer(text, delimiters);

        while (tokenizer.hasMoreTokens()) {
            String word = tokenizer.nextToken().toLowerCase(); // –ü–µ—Ä–µ–≤–æ–¥–∏–º–æ —É –Ω–∏–∂–Ω—ñ–π —Ä–µ–≥—ñ—Å—Ç—Ä –¥–ª—è –±–µ–∑–ø–µ–∫–∏
            
            // –û—Å–∫—ñ–ª—å–∫–∏ –≤ —É–º–æ–≤—ñ —Å–∫–∞–∑–∞–Ω–æ, —â–æ —Ç–µ–∫—Å—Ç —ñ–∑ –º–∞–ª–∏—Ö –ª–∞—Ç–∏–Ω—Å—å–∫–∏—Ö –ª—ñ—Ç–µ—Ä, 
            // –¥–æ–¥–∞—Ç–∫–æ–≤—É –ø–µ—Ä–µ–≤—ñ—Ä–∫—É –Ω–∞ –ª—ñ—Ç–µ—Ä–∏ –º–æ–∂–Ω–∞ –æ–ø—É—Å—Ç–∏—Ç–∏.
            
            if (isLexicographicallySorted(word)) {
                lexicographicalWords.add(word);
            }
        }
        
        return lexicographicalWords;
    }

    /**
     * –ü–µ—Ä–µ–≤—ñ—Ä—è—î, —á–∏ —Ä–æ–∑–º—ñ—â–µ–Ω—ñ –ª—ñ—Ç–µ—Ä–∏ –≤ —Å–ª–æ–≤—ñ –≤ –ª–µ–∫—Å–∏–∫–æ–≥—Ä–∞—Ñ—ñ—á–Ω–æ–º—É –ø–æ—Ä—è–¥–∫—É 
     * (—Ç–æ–±—Ç–æ –∫–æ–∂–Ω–∞ –Ω–∞—Å—Ç—É–ø–Ω–∞ –ª—ñ—Ç–µ—Ä–∞ –±—ñ–ª—å—à–∞ –∞–±–æ —Ä—ñ–≤–Ω–∞ –ø–æ–ø–µ—Ä–µ–¥–Ω—ñ–π).
     * @param word –°–ª–æ–≤–æ –¥–ª—è –ø–µ—Ä–µ–≤—ñ—Ä–∫–∏.
     * @return true, —è–∫—â–æ –ª—ñ—Ç–µ—Ä–∏ –≤—ñ–¥—Å–æ—Ä—Ç–æ–≤–∞–Ω—ñ, false - —ñ–Ω–∞–∫—à–µ.
     */
    public static boolean isLexicographicallySorted(String word) {
        // –°–ª–æ–≤–∞ –∑ 0 –∞–±–æ 1 –ª—ñ—Ç–µ—Ä–æ—é –∑–∞–≤–∂–¥–∏ –≤–≤–∞–∂–∞—é—Ç—å—Å—è –≤—ñ–¥—Å–æ—Ä—Ç–æ–≤–∞–Ω–∏–º–∏.
        if (word == null || word.length() <= 1) {
            return true;
        }

        // –ü–µ—Ä–µ–≤—ñ—Ä—è—î–º–æ –∫–æ–∂–Ω—É –ø–∞—Ä—É —Å—É—Å—ñ–¥–Ω—ñ—Ö —Å–∏–º–≤–æ–ª—ñ–≤
        for (int i = 0; i < word.length() - 1; i++) {
            // –Ø–∫—â–æ –ø–æ—Ç–æ—á–Ω–∏–π —Å–∏–º–≤–æ–ª –±—ñ–ª—å—à–∏–π –∑–∞ –Ω–∞—Å—Ç—É–ø–Ω–∏–π, —Ç–æ –ø–æ—Ä—è–¥–æ–∫ –ø–æ—Ä—É—à–µ–Ω–æ
            if (word.charAt(i) > word.charAt(i + 1)) {
                return false;
            }
        }

        return true;
    }
}
